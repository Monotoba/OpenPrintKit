{
  "type": "printer",
  "name": "Longer LK5 Pro (Marlin)",
  "firmware": "Marlin 2.x",
  "kinematics": "cartesian",
  "nozzle_diameter": 0.4,
  "filament_diameter": 1.75,
  "build_volume": [300, 300, 400],
  "heated_bed": true,
  "auto_bed_leveling": false,
  "max_nozzle_temperature": 260,
  "max_bed_temperature": 100,
  "max_feedrate_xyz": [150, 150, 5],
  "max_acceleration_xyz": [1000, 1000, 100],
  "steps_per_mm_xyz": [80, 80, 400],
  "machine_gcode_flavor": "Marlin",
  "gcode_start": [
    "M117 Initializing LK5 Pro print...",
    "G28 ; Home all axes",
    "G1 Z5 F5000 ; lift nozzle",
    "M104 S{material_print_temperature_layer_0} ; set nozzle temp",
    "M140 S{material_bed_temperature_layer_0} ; set bed temp",
    "M190 S{material_bed_temperature_layer_0} ; wait for bed",
    "M109 S{material_print_temperature_layer_0} ; wait for nozzle",
    "G92 E0",
    "G1 Y0 X0 F3000",
    "G1 Z0.3 F1000",
    "G1 X60 E9 F1000",
    "G1 X100 F5000",
    "G92 E0"
  ],
  "gcode_end": [
    "M104 S0 ; turn off hotend",
    "M140 S0 ; turn off bed",
    "G91 ; relative moves",
    "G1 E-2 F300",
    "G1 Z10 F3000",
    "G90",
    "G1 X0 Y300 F3000 ; park head",
    "M84 ; disable motors",
    "M117 Print complete!"
  ],
  "comments": "Baseline Longer LK5 Pro profile for Marlin firmware. Includes safe temperature limits and startup/teardown G-code. Compatible with Orca and Cura slicers via OpenPrintKit."
}
